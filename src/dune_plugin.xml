<dune_plugin>
  <name>myiptv</name>
  <caption>myIPTV</caption>
  <auto_resume>
    <enable>yes</enable>
  </auto_resume>
  <entry_points>
    <entry_point>
        <parent_media_url>root://tv</parent_media_url>
        <media_url>tv_group_list</media_url>
        <caption>myIPTV</caption>
        <icon_url>gui_skin://large_icons/iptv.aai</icon_url>
        <actions>
          <key_enter>
            <type>plugin_open_folder</type>
          </key_enter>
          <key_play>
            <type>plugin_tv_play</type>
          </key_play>
        </actions>
        <ip_address_required>yes</ip_address_required>
        <show_cookie_name>show_tv</show_cookie_name>
        <show_by_default>yes</show_by_default>
    </entry_point>
    <entry_point>
        <parent_media_url>setup://applications</parent_media_url>
        <media_url>setup</media_url>
        <caption>myIPTV</caption>
        <icon_url>gui_skin://large_icons/iptv.aai</icon_url>
        <actions>
          <key_enter>
            <type>plugin_open_folder</type>
          </key_enter>
        </actions>
    </entry_point>
  </entry_points>
  <type>php</type>
  <params>
     <program>myiptv.php</program>
  </params>
 <version_index>7</version_index>
  <version>2.2.2</version>
  <check_update>
    <schema>1</schema>
    <url>http://blog-isayev-org-ua.googlecode.com/svn/trunk/dune_plugin_myiptv/update.txt</url>
    <timeout>0</timeout>
  </check_update>
</dune_plugin>
